{
  "name": "node-api-service",
  "version": "1.0.1",
  "description": "Personal boilerplate for Node projects",
  "main": "dist/index.js",
  "scripts": {
    "flow": "flow",
    "test": "ava src/**/*.test.js",
    "test:watch": "npm test -- --watch",
    "test:int": "ava src/integration-tests/**/*.int.js",
    "build": "babel src/ --out-dir dist/ --ignore src/**/*.spec.js,src/**/*.test.js",
    "dev": "concurrently \"npm run build -- --watch\" \"nodemon dist/index.js\""
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "node",
    "fastify",
    "api"
  ],
  "author": "Patrick Metzdorf <patrick.metzdorf@hey.com>",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.0.0",
    "dotenv": "^16.0.1",
    "fastify": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.18.9",
    "@babel/core": "7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "7.18.9",
    "@babel/preset-flow": "7.18.6",
    "ava": "4.3.1",
    "babel-eslint": "10.1.0",
    "concurrently": "7.2.2",
    "eslint": "8.20.0",
    "eslint-config-koa": "2.0.2",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-ava": "13.2.0",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-standard": "4.1.0",
    "flow-bin": "0.182.0",
    "nodemon": "2.0.19",
    "pino-pretty": "8.1.0",
    "prettier": "2.7.1",
    "supertest": "6.2.4"
  }
}
